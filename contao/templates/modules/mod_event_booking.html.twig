{% extends '@Contao/block_unsearchable' %}

{% block content %}

    {% if caseText|default %}
        <div class="cebb-case-text">
            {{ caseText }}
        </div>
    {% endif %}

    {% if messages|default %}
        <div class="cebb-messages">
            {{ messages|raw }}
        </div>
    {% endif %}

    {% if form|default and canRegister|default(false) %}
        {{ form|raw }}
    {% endif %}

{% endblock %}
